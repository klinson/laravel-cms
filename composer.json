{
    "name": "klinson/laravel-cms",
    "description": "The cms depend on laravel/lavavel and encore/laravel-admin",
    "keywords": ["framework", "laravel", "laravel-cms", "laravel-admin", "cms"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "dingo/api": "2.0.0-alpha2",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "~3.3",
        "laravel-admin-ext/backup": "^1.0",
        "laravel-admin-ext/config": "^1.0",
        "laravel-admin-ext/media-manager": "^1.0",
        "laravel-admin-ext/media-player": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "liyu/dingo-serializer-switch": "^0.3.0",
        "maatwebsite/excel": "~2.1.0",
        "mews/captcha": "^2.2",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-query-logger": "^1.0",
        "overtrue/laravel-wechat": "~4.0",
        "predis/predis": "^1.1",
        "tymon/jwt-auth": "1.0.0-rc.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orangehill/iseed": "^2.6",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
